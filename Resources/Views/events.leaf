#extend("base")

#export("title") { Events }

#export("content") {
    <h2>Events</h2>

    <h3>Active this week</h3>

    <ul>
    #loop(activeEvents, "event") {
        <li><a href="/events/#(event.id)">#(event.name)</a></li>
    }
    </ul>

    <h3>Previous events</h3>

    <ul>
    #loop(pastEvents, "event") {
        <li><a href="/events/#(event.id)">#(event.name)</a></li>
    }
    </ul>

    <p>None</p>
}
